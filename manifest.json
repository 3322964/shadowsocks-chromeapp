{
    "name":"shadowsocks",
    "description":"shadowsocks client for Chrome.",
    "version":"0.1",
    "manifest_version":2,
    "app":{
        "background":{
            "scripts":["background.js", "t.js"]
        }
    },
    "icons":{
        "16":"icon-16.png",
        "128":"icon-128.png"
    },
     "sockets": {
        "udp": {
          "bind": "*",
          "send": "*"
        },
        "tcp": {
          "connect": "*"
        },
        "tcpServer": {
          "listen": "*"
        }
      },
    "permissions":[
        "storage",
        {
            "socket":[
                "tcp-connect",
                "tcp-listen"
            ]
        }
    ]
}